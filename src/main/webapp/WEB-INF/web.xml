<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>New-match-servlet</servlet-name>
        <jsp-file>/new-match.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>New-match-servlet</servlet-name>
        <url-pattern>/new-match</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>matches</servlet-name>
        <jsp-file>/matches.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>matches</servlet-name>
        <url-pattern>/matches</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>match-score</servlet-name>
        <jsp-file>/match-score.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>match-score</servlet-name>
        <url-pattern>/match-score</url-pattern>
    </servlet-mapping>

</web-app>
